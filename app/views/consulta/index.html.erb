<% @consulta.each do |consultum| %>
  <tr>
    <td><%= consultum.id_paciente %></td>
    <td><%= consultum.Descricao %></td>
    <td><%= consultum.Tipo_consulta %></td>
    <td><%= consultum.Exames %></td>
    <td><%= consultum.Exame_fisico %></td>
    <td><%= consultum.Cirurgia %></td>
    <td><%= consultum.Diagnostico %></td>
    <td><%= consultum.Convenio %></td>
    <td><%= consultum.Tipo_plano %></td>
    <td><%= consultum.Data_consulta %></td>
    <td><%= consultum.Status %></td>
    <td><%= consultum.id_consulta %></td>
    <td><%= consultum.id_medico %></td>
    <td><%= consultum.id_systemUser %></td>
    <td><%= link_to 'Show', consultum %></td>
    <td><%= link_to 'Edit', edit_consultum_path(consultum) %></td>
    <td><%= link_to 'Destroy', consultum, :confirm => 'Are you sure?', :method => :delete %></td>
